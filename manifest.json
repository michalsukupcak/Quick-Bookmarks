{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "Quick Bookmarks",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": "images/icon48.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "bookmarks",
        "chrome://favicon/",
        "tabs"
    ]
}
